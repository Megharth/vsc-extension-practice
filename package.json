{
	"name": "practice",
	"displayName": "practice",
	"description": "A practice app",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:practice.colorView",
		"onCommand:practice.helloWorld",
		"onCommand:practice.newCommand"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "practice",
					"title": "Practice Extension",
					"icon": "src/assets/dashboard-dark.png"
				}
			]
		},
		"views": {
			"practice": [
				{
					"id": "practice.colorView",
					"name": "Practice Colorview",
					"icon": "src/assets/dashboard-dark.png",
					"contextualTitle": "Practice Extension",
					"type": "webview"
				}
			]
		},
		"commands": [
			{
				"command": "practice.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "practice.newCommand",
				"title": "New command"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "npm-run-all compile:*",
		"compile:extension": "tsc -p ./",
		"compile:views": "webpack --mode development",
		"watch": "npm-run-all -p watch:*",
		"watch:extension": "tsc -watch -p ./",
		"watch:views": "webpack --watch --mode development",
		"package": "webpack --mode production --devtool hidden-source-map --config ./build/node-extension.webpack.config.js",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/react": "^17.0.0",
		"@types/react-dom": "^17.0.0",
		"@types/vscode": "^1.52.0",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"css-loader": "^5.0.1",
		"eslint": "^7.15.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"npm-run-all": "^4.1.5",
		"style-loader": "^2.0.0",
		"ts-loader": "^8.0.12",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1",
		"webpack": "^5.10.0",
		"webpack-cli": "^4.2.0"
	},
	"dependencies": {
		"react": "^17.0.1",
		"react-dom": "^17.0.1"
	}
}
